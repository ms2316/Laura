function [out1 , out2] = qsort(ind, cf, l, r)
    k = l;
    j = r;
    mid = ind( floor( (l+r)/2 ) );
    while (k <= l) do
        while (ind(k) < mid)
            k = k + 1;
        end
        while (a(j) > mid)
            j = j - 1;
        end
        if (k <= j)
            w = a(k);
            a(k) = a(j);
            a(j) = w;
            k = k + 1;
            j = j -1;
        end
    end